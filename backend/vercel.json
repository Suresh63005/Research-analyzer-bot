{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/pdfanalyze": {
      "maxDuration": 10,
      "memory": 256,
      "maxLambdaSize": "20mb"
    }
  }
}
